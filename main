from kivymd.app import MDApp
from kivy.uix.screenmanager import ScreenManager
from kivy.lang import Builder
from kivy.core.window import Window
Window.size = 360,640

class Function(ScreenManager):
    pass

class Main(MDApp):
    Builder.load_file('layout.kv')
    def build(self):
        return Function()

Main().run()
